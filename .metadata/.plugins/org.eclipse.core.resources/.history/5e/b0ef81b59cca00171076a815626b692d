
public class Particle {
	
	double loc[];
	double vel[];
	double acc[];
	double col;
	double life;
		
	public Particle(double[] l_,double[] v_, double[] a_, double c_){
		loc = l_;
		vel = v_;
		acc = a_;
		col = c_;
	}
	
	
	public double[] update(){
		loc = add(loc,vel);//update location 
		vel = add(vel,acc);//update velocity
		life--;//decrement lifespan 
		return loc;
	}
	
	public double[] add()
	
	
	
	

	
	
}
